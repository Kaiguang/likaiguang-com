{
  "name": "likaiguang.com",
  "description": "Kai's personal website",
  "version": "2.0.0",
  "author": "Kaiguang Li <lkaiguang@gmail.com>",
  "scripts": {
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "clean": "gatsby clean",
    "deploy": "npm run build && gatsby-plugin-s3 deploy",
    "dev": "gatsby develop -H \"0.0.0.0\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src",
    "serve": "gatsby serve",
    "start": "npm run dev",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^3.13.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^3.13.0",
    "gatsby-plugin-manifest": "3.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-remark-copy-linked-files": "^4.10.0",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-remark-prismjs": "^5.10.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-remark": "^4.10.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kaiguang/likaiguang.com.git"
  },
  "bugs": {
    "url": "https://github.com/Kaiguang/likaiguang.com/issues"
  },
  "license": "(CC-BY-4.0 OR MIT)"
}
